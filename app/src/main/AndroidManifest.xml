<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imuhelper">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.ScoreList"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".activities.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.TermListActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activities.LogIn"
            android:screenOrientation="portrait"/>
        <activity android:name=".activities.AddTermActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activities.AddCourseActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>